<li class="last-users">
	<div class="foto">
		<p><a href="{name-url}">{name-colored}</a> [user-group=1](Admin)[/user-group]</p>
		<img src="{foto}"[online] class="onl"[/online] alt="" />
		<br /><span>{news-num}/{comm-num}</span>
	</div>
	<div class="info">
		[news]
		<p>Son Yazısı: <a href="{news-link}">{news-title}</a></p>
		<p>Tarih: {news-date=d.m.Y}</p>
		[/news]
		<i>Son Ziyareti: {last-date=d.m.Y}<br /> 
		Kayıt Tarihi: {reg-date=d.m.Y}</i>
	</div>
	<div style="clear:both"></div>
</li>
